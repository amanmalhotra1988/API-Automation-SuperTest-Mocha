{
  "name": "solar-fintech-api-automation",
  "version": "1.0.0",
  "description": "A project to start with REST API Automation using Supertest, Chai, Mocha",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -r ts-node/register --spec test/**/*.test.ts --timeout 60000 -r mochawesome/register --reporter mochawesome --parallel",
    "report": "cd mochawesome-report && mochawesome.html"
  },
  "keywords": [
    "api-automation",
    "supertest",
    "mocha",
    "chai"
  ],
  "author": "Aman Malhotra",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^17.0.17",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.6",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "nock": "^13.2.4",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "node": "^17.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
